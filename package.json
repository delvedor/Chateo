{
  "name": "chateo",
  "version": "0.1.0",
  "description": "A simple p2p lan chat.",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "build:es6": "apv update build && babel es6 --watch --out-dir ./",
    "build:win": "apv update build && npm run clean && electron-packager \"./\" \"Chateo\" --out=\"buildOS/win\" --platform=\"win32\" --arch=\"x64\" --version=\"0.34.0\" --ignore=\"(buildOS|node_modules/\\.bin|node_modules/electron-builder|node_modules/electron-packager|node_modules/electron-prebuilt|es6|README.md)\" app-version=\"0.0.0\" --asar",
    "clean": "rm -rf buildOS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://delvedor@github.com/delvedor/Chateo.git"
  },
  "keywords": [
    "app",
    "chat",
    "p2p"
  ],
  "author": "Tomas Della Vedova - @delvedor (http://delved.org)",
  "license": "GPLv2",
  "bugs": {
    "url": "https://github.com/delvedor/Chateo/issues"
  },
  "private": true,
  "homepage": "https://github.com/delvedor/Chateo",
  "devDependencies": {
    "electron-builder": "^2.0.1",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.34.0"
  },
  "dependencies": {
    "airswarm": "^1.1.0",
    "appversion": "^1.0.1",
    "duplex-json-stream": "^1.0.1",
    "lookup-multicast-dns": "^1.0.3",
    "moment": "^2.10.6",
    "stream-set": "^1.1.0"
  }
